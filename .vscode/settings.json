{
    "githubPullRequests.ignoredPullRequestBranches": [
        "master"
    ],
    "C_Cpp.updateChannel": "Insiders",
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "gitlens.blame.avatars": false,
    "editor.minimap.enabled": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.highlightModifiedTabs": true,
    "html.format.wrapLineLength": 0,
    "files.associations": {
        "*.json": "json"
    },
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "eslint.alwaysShowStatus": true,
    "auto-rename-tag.activationOnLanguage": ["*"],
    "workbench.startupEditor": "none",
    "gitlens.views.contributors.files.layout": "tree",
    "git.autofetch": true,
    "workbench.iconTheme": "vscode-icons",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "eslint.format.enable": true,
    "gitlens.gitCommands.skipConfirmations": ["fetch:command", "stash-push:command", "switch:command", "push:command", "stash-apply:command"],
    "debug.javascript.suggestPrettyPrinting": false,
    "editor.linkedEditing": true,
    "cSpell.language": "en-GB",
    "cSpell.userWords": ["alienrpg", "dialog", "fantaistic", "Kastor", "maneuvers", "roleplay", "SPRITUALITY", "whihc"],
    "cSpell.enableFiletypes": ["!css", "!javascript", "!scss"],
    "liveSassCompile.settings.formats": [
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "/css"
        }
    ],
    "liveSassCompile.settings.excludeList": ["/.vscode/**", "/**/node_modules/**","/.github/**"],
    "liveSassCompile.settings.watchOnLaunch": true,
    "vsicons.dontShowNewVersionMessage": true,
    "json.format.enable": false,
    "css.hover.references": false,
    "html.hover.references": false,
    "less.hover.references": false,
    "scss.hover.references": false,
    "vscode-edge-devtools.mirrorEdits": true,
    "liveSassCompile.settings.forceBaseDirectory": "/scss",
    "liveSassCompile.settings.generateMap": true,
    "liveSassCompile.settings.autoprefix": [
      

      "> 1%",
      "last 2 versions"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "problems.decorations.enabled": false,
    "vscode-edge-devtools.webhint": false,
    "editor.hover.delay": 1000,
    "vscode-edge-devtools.fallbackRevision": "@a41122be052fe9616f0def5fe6842fa942930d46",
    "errorLens.enabled": false,
    "projectManager.tags": ["Alien", "Symbaroum", "RoS"],
    "window.commandCenter": true,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "liveSassCompile.settings.useNewCompiler": true,
    "gitlens.advanced.repositorySearchDepth": 3,
    "editor.fontSize": 15,
    "prettier.useTabs": true,
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {},
    "githubPullRequests.pullBranch": "never",
    "[handlebars]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
}
